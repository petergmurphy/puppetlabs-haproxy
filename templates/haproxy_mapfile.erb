<%- @mappings.each do |mapping| -%>
<%- if mapping.is_a?(Hash) -%>
<%- mapping.sort.map do |key, val| -%>
<%= key -%> <%= val %>
<%- end -%>
<%- elsif mapping.is_a?(String) -%>
<%= mapping %>
<%- else -%>
<% scope.function_fail(["Haproxy::Mapfile[#{@mapfile_name}]: $mappings array must contain only Hashes or Strings"]) -%>
<%- end -%>
<%- end -%>
